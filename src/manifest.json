{
    "name": "KeepLearning",
    "version": "1.0",
    "description": "Extension",
    "permissions": ["storage","tabs","<all_urls>"],
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
    "browser_action": {
      "default_popup": "popup.html"
    },
    "options_ui": {
      "open_in_tab": true,
      "page": "options.html"
    },
    "manifest_version": 2
  }