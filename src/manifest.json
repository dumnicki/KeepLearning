{
    "name": "KeepLearning",
    "version": "1.0",
    "description": "Extension",
    "permissions": ["declarativeContent","storage"],
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "browser_action": {
      "default_popup": "popup.html"
    },
    "options_ui": {
      "open_in_tab": true,
      "page": "options.html"
    },
    "manifest_version": 2
  }